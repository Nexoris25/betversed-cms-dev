{
  "collectionName": "components_shared_seos",
  "info": {
    "displayName": "seo"
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "maxLength": 60,
      "required": true
    },
    "metaDescription": {
      "type": "text",
      "maxLength": 160,
      "required": true
    },
    "ogImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "twitterCardType": {
      "type": "enumeration",
      "default": "summary_large_image",
      "enum": [
        "summary",
        "summary_large_image",
        "app",
        "player"
      ]
    },
    "noIndex": {
      "type": "boolean"
    },
    "noFollow": {
      "type": "boolean"
    }
  },
  "config": {}
}
