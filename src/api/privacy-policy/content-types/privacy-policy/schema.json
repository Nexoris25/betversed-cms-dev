{
  "kind": "singleType",
  "collectionName": "privacy_policies",
  "info": {
    "singularName": "privacy-policy",
    "pluralName": "privacy-policies",
    "displayName": "Privacy Policy"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pageTitle": {
      "type": "string",
      "maxLength": 80,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "pageTitle",
      "required": true
    },
    "seoMeta": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "lastUpdated": {
      "type": "date"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    }
  }
}
