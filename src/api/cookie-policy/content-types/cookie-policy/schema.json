{
  "kind": "singleType",
  "collectionName": "cookie_policies",
  "info": {
    "singularName": "cookie-policy",
    "pluralName": "cookie-policies",
    "displayName": "Cookie Policy"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "maxLength": 80,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "lastUpdated": {
      "type": "date"
    },
    "seoMeta": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "Content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "FAQs": {
      "type": "component",
      "component": "shared.faqs-content",
      "repeatable": false
    }
  }
}
